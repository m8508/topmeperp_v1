@model topmeperp.Models.ContractModels
<table class="table">
    <tr>
        <th>
            NO.
        </th>
        <th>
            合約類型
        </th>
        <th>
            合約名稱
        </th>
        <th>
            廠商名稱
        </th>
        <th>
            採購項目
        </th>
        <th>
            合約品項個數
        </th>
        <th>
            功能
        </th>

    </tr>

    @foreach (var item in Model.contractItems)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TYPE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CONTRACT_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SUPPLIER_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FORM_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ITEM_ROWS)
            </td>
            <td>
                <a href="/Estimation/AddEST/@item.CONTRACT_ID" target="_blank">新增估驗單</a>
            </td>
        </tr>
    }

</table>

