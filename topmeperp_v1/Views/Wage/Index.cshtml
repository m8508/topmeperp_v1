@{
    ViewBag.Title = "備標作業-工率表下載作業";
    Layout = "~/Views/Shared/_MenuLayout.cshtml";
}

<div class="container">
    <h2>工率下載與上傳作業</h2><br />
    <div class="row col-md-12">
        <label for="projectId">專案編號:</label>
        <input type="text" name="projectid" id="projectid" value="@ViewBag.projectid" />
    </div>
    <div class="row col-md-12">
        <label for="projectId">專案編號:</label>
        <input type="text" name="projectid" id="projectid" value="@ViewBag.projectid" />
    </div><br />
    <div class="row col-md-12">
        <label>工率填寫表連結</label>
        <a href='@("/UploadFile/"+ViewBag.projectid+"/"+ViewBag.projectid + "_工率.xlsx")'>下載</a>
    </div><br />
    <div class="row col-md-12">
        <form id="UploadForm" name="UploadForm" action="~/Wage/uploadWageTable" method="post" enctype="multipart/form-data">
            <label for="file">工率表匯入:</label>
            <input type="hidden" name="projectid" id="projectid" value="@ViewBag.projectid" />
            <input type="file" name="fileWage" id="fileWage" />
            <input id="btnUploadfile" name="btnUploadfile" type="submit" class="btn-primary" value="匯入檔案" />
        </form>
    </div>
        </div>
   
