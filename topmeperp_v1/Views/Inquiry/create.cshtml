@{
    ViewBag.Title = "備標作業-空白詢價單 ";
    Layout = "~/Views/Shared/_MenuLayout.cshtml";
}
<div class="jumbotron">
    <div class="row">
        <div class="col-md-12"><p class="text-center"><h2>協成水電工程事業有限公司</h2></p></div>
        <div class="col-md-12"><p class="text-center"><h3>採購詢價單</h3></p></div>
    </div>
    <!-- Reg-Form new { enctype = "multipart/form-data" } -->
    @using (Html.BeginForm("Create", "Inquiry", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <form name="FormSearchProject" id="FormSearchProject" action="" method="Post">
            <div class="row">
                <div class="form-group">
                    <label for="inputprojectname" class="col-md-2 control-label">專案名稱</label>
                    <div class="col-md-4">
                        @Request["prjId"]
                    </div>
                </div>
                <div class="form-group">
                    <label for="inputsuppliername" class="col-md-2 control-label">廠商名稱 </label>
                    <div class="col-md-4 ">
                        <input type="text" class="form-control" id="inputsuppliername">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group">
                    <label for="inputitem" class="col-md-2 control-label">採購項目</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputitem">
                    </div>
                </div>
                <div class="form-group">
                    <label for="inputcontact" class="col-md-2 control-label">聯絡人</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputcontact">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group">
                    <label for="inputowner" class="col-md-2 control-label">承辦人</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputowner">
                    </div>
                </div>
                <div class="form-group">
                    <label for="inputemail" class="col-md-2 control-label">E-mail</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputemail">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group">
                    <label for="inputphone" class=col-md-2 control-label">聯絡電話</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputphone">
                    </div>
                </div>
                <div class="form-group">
                    <label for="inputdeteline" class="col-md-2 control-label">報價期限</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputdateline">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group">
                    <label for="inputowneremail" class="col-md-2 control-label">承辦人E-mail</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputowneremail">
                    </div>
                </div>
                <div class="form-group">
                    <label for="inputformnumber" class="col-md-2 control-label">詢價單編號</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputformnumber">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group">
                    <label for="inputownerfax" class="col-md-2 control-label">承辦人傳真</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="inputownerfax">
                    </div>
                </div>
            </div>
            <div class="row" style="padding-top:10px;">
                <div class="col-md-12">
                    <input class="btn btn-info" id="Submit2" type="submit" value="產生詢價單" />
                </div>
            </div>
        </form>
    }
</div>
<!--被選取標單項目列表區域-->
@{
    Html.RenderPartial("ProjectFormList");
}

<script>
        $(document).ready(function () {
            $(function () {
                $('#due_date').datetimepicker({
                    format: 'YYYY/MM/DD'
                });
            });
        })
</script>


