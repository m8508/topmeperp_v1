
@{
    ViewBag.Title = "備標作業-新增詢價單";
    Layout = "~/Views/Shared/_MenuLayout.cshtml";
}
<!--標單項目搜尋區條件區域-->
<div class="jumbotron">
    <div class="row">
        <div class="col-md-8">
            <form name="FormSearchProject" id="FormSearchProject" action="Search" method="Post">
                九宮格&nbsp&nbsp : <input id="textCode1" name="textCode1" type="text" />
                主系統 : <input id="textSystemMain" name="textSystemMain" type="text" /><br />
                次九宮格: <input id="textCode2" name="textCode2" type="text" />
                次系統: <input id="textSystemSub" name="textSystemSub" type="text" /> &nbsp&nbsp&nbsp&nbsp
                <input id="Submit1" type="submit" value="查詢" />
            </form>
            <div id="divProcessing">
                <img src="~/Content/ajax-loader.gif">
            </div>
        </div>
    </div>
    </div>
 <!--詢價單樣本資料區域-->
 <!--標單項目列表區域-->
     <div class="container">
     @ViewBag.SearchResult
     @if (null != Model)
            {
              Html.RenderPartial("RFQ");
            }
     </div>
     <script type="text/javascript">
             $(document).ready(function () {
             // Hide the "busy" Gif at load:
             $("#divProcessing").hide();
             // Attach click handler to the submit button:
             $('#login').click(function () {
             $("#divProcessing").show();
               });
              });
      </script>




